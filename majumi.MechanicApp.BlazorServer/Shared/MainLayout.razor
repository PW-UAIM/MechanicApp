@inherits LayoutComponentBase
@inject IJSRuntime JSRuntime

<div>
    <NavMenu />
</div>

@Body


@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "https://code.jquery.com/jquery-3.4.1.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/carservice/lib/easing/easing.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/carservice/lib/waypoints/waypoints.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/carservice/lib/counterup/counterup.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/carservice/lib/owlcarousel/owl.carousel.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/carservice/js/main.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/majumiservice/js/bootstrap.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/majumiservice/js/click-scroll.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/majumiservice/js/counter.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/majumiservice/js/custom.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/majumiservice/js/jquery.min.js");
            await JSRuntime.InvokeAsync<IJSObjectReference>("import", "/majumiservice/js/jquery.sticky.js");
        }
    }
}